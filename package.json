{
  "name": "turboself-api",
  "version": "0.1.2",
  "description": "A wrapper around Turboself internal API.",
  "repository": "github:raphckrman/turboself-api",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "init": "npm install",
    "build": "npm install && node scripts/build && gen-esm-wrapper dist/index.js dist/index.mjs",
    "compile": "node scripts/build && gen-esm-wrapper dist/index.js dist/index.mjs",
    "lint": "eslint lib",
    "lintFix": "eslint lib --fix",
    "devTest": "ts-node scripts/tests/client.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "turboself",
    "api",
    "wrapper",
    "student",
    "France"
  ],
  "author": "RaphaÃ«l <raphckrman> (https://github.com/raphckrman)",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@inquirer/prompts": "^4.1.0",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@types/node-forge": "^1.3.10",
    "@types/pako": "^2.0.3",
    "@types/set-cookie-parser": "^2.4.7",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "gen-esm-wrapper": "^1.1.3",
    "release-it": "^17.0.1",
    "terser": "^5.26.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
